install.packages("renv")
renv::init()
renv::init()
if (file.exists("renv.lock")) {
Sys.chmod("renv.lock", mode = "0644")   # clear read-only bit
unlink("renv.lock")                     # or keep a copy: file.rename("renv.lock","old.lock")
}
unlink("renv",  recursive = TRUE, force = TRUE)
renv::init(force = TRUE, prompt = FALSE)
renv::init(force = TRUE)
